<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension-point id="svgGenerators" name="SVG Generators" schema="schema/svgGenerators.exsd"/>
   <extension
         point="de.cau.cs.kieler.kiml.layoutProviders">
      <layoutOption
            appliesTo="nodes"
            default="null"
            id="klighd.tooltip"
            name="Tooltip"
            type="string">
      </layoutOption>
   </extension>
   <extension
         point="de.cau.cs.kieler.klighd.extensions">
      <exporter
            class="de.cau.cs.kieler.klighd.piccolo.export.BitmapExporter"
            extension="png"
            id="de.cau.cs.kieler.klighd.piccolo.exporter.png"
            subFormat="png"
            supports_tiling="true">
      </exporter>
      <exporter
            class="de.cau.cs.kieler.klighd.piccolo.export.BitmapExporter"
            extension="jpeg"
            id="de.cau.cs.kieler.klighd.piccolo.exporter.jpeg"
            subFormat="jpeg"
            supports_tiling="true">
      </exporter>
      <exporter
            class="de.cau.cs.kieler.klighd.piccolo.export.BitmapExporter"
            extension="bmp"
            id="de.cau.cs.kieler.klighd.piccolo.exporter.bmp"
            subFormat="bmp"
            supports_tiling="true">
      </exporter>
      <offscreenRenderer
            class="de.cau.cs.kieler.klighd.piccolo.export.BitmapOffscreenRenderer"
            id="de.cau.cs.kieler.klighd.piccolo.export.BitmapOffscreenRenderer"
            supportedFormats="bmp, jpeg, png">
      </offscreenRenderer>
      <offscreenRenderer
            class="de.cau.cs.kieler.klighd.piccolo.export.SVGOffscreenRenderer"
            id="de.cau.cs.kieler.klighd.piccolo.export.SVGOffscreenRenderer"
            supportedFormats="svg">
      </offscreenRenderer>
   </extension>

</plugin>
