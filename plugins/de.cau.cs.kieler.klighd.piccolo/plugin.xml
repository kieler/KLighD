<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension-point id="svgGenerators" name="SVG Generators" schema="schema/svgGenerators.exsd"/>
   <extension
         point="de.cau.cs.kieler.klighd.extensions">
      <exporter
            class="de.cau.cs.kieler.klighd.piccolo.export.BitmapExporter"
            description="Bitmap image (.bmp)"
            extension="bmp"
            id="de.cau.cs.kieler.klighd.piccolo.exporter.bmp"
            subFormat="bmp"
            supportsTiling="true">
      </exporter>
      <exporter
            class="de.cau.cs.kieler.klighd.piccolo.export.BitmapExporter"
            description="JPEG image (.jpeg)"
            extension="jpeg"
            id="de.cau.cs.kieler.klighd.piccolo.exporter.jpeg"
            subFormat="jpeg"
            supportsTiling="true">
      </exporter>
      <exporter
            class="de.cau.cs.kieler.klighd.piccolo.export.BitmapExporter"
            description="Portable Network Graphics (.png)"
            extension="png"
            id="de.cau.cs.kieler.klighd.piccolo.exporter.png"
            subFormat="png"
            supportsTiling="true">
      </exporter>
      <offscreenRenderer
            class="de.cau.cs.kieler.klighd.piccolo.export.BitmapOffscreenRenderer"
            id="de.cau.cs.kieler.klighd.piccolo.export.BitmapOffscreenRenderer"
            supportedFormats="bmp, jpeg, png">
      </offscreenRenderer>
      <offscreenRenderer
            class="de.cau.cs.kieler.klighd.piccolo.export.SVGOffscreenRenderer"
            id="de.cau.cs.kieler.klighd.piccolo.export.SVGOffscreenRenderer"
            supportedFormats="svg">
      </offscreenRenderer>
      <!-- exportBranding
            class="de.cau.cs.kieler.klighd.piccolo.export.AuthorConfidentialExportBranding"
            id="de.cau.cs.kieler.klighd.piccolo.export.AuthorConfidentialExportBranding"
            supportedFormats="printout, bmp, jpeg, png, de.cau.cs.kieler.klighd.piccolo.svggen.batik, de.cau.cs.kieler.klighd.piccolo.svggen.freeHEPSemantic">
      </exportBranding -->
   </extension>

</plugin>
