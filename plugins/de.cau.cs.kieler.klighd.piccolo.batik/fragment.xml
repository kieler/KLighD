<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<fragment>
   <extension
         point="de.cau.cs.kieler.klighd.piccolo.svgGenerators">
      <svgGenerator
            class="de.cau.cs.kieler.klighd.piccolo.batik.BatikSVGGraphics"
            id="de.cau.cs.kieler.klighd.piccolo.svggen.batik">
      </svgGenerator>
   </extension>
   <extension
         point="de.cau.cs.kieler.klighd.extensions">
      <exporter
            class="de.cau.cs.kieler.klighd.piccolo.export.SVGExporter"
            description="Scalable Vector Graphics (.svg) powered by Batik"
            extension="svg"
            id="de.cau.cs.kieler.klighd.piccolo.export.batik.svg"
            subFormat="de.cau.cs.kieler.klighd.piccolo.svggen.batik">
      </exporter>
      <exporter
            class="de.cau.cs.kieler.klighd.piccolo.batik.BatikPDFGraphics"
            description="Portable Document Format (.pdf) powered by Batik"
            extension="pdf"
            id="de.cau.cs.kieler.klighd.piccolo.export.batik.pdf">
      </exporter>
   </extension>


</fragment>
